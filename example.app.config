    <Properties>
      <!-- Path to the NERVA CLI tools -->
      <ns1:ToolsPath Value="/home/steve/Projects/nerva/bin/" />
      <!-- Check for updates to the CLI tools on startup? -->
      <ns2:CheckForUpdateOnStartup Value="True" />
      <!-- Log the RPC requests and responses? Debug purposes only. Will blow out your log file -->
      <ns2:LogRpcTraffic Value="False" />
      <!-- Address to mine to -->
      <ns1:WalletAddress Value="NV1tXu3czVbaBWYcKg28X4P2mHrJZMnS8R3TshtYgJVrfA9XLrKRKzVcjaPLgjNiwzH3DNTeywRTURDJUY59a5Lb2vWSG9s5q" />
      <ns3:Daemon>
        <Fields />
